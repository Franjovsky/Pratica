	<queryString>
		<![CDATA[select ano, mes, produto, sum(mov_qtde) qtde
	from(
		select cast(extract(year from mov_data) as integer) Ano, cast(extract(month from mov_data) as integer)Mes, produto.descricao_produto produto, cast(mov_qtde as integer)mov_qtde
		from movimentacao m
		inner join produto on produto.id_produto=m.id_produto
		
		)a

